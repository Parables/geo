{
    "name": "parables/geo",
    "description": "This is my package geo",
    "keywords": [
        "Parables",
        "laravel",
        "geo"
    ],
    "homepage": "https://github.com/parables/geo",
    "license": "MIT",
    "authors": [
        {
            "name": "parables",
            "email": "38154990+Parables@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "kalnoy/nestedset": "^6.0",
        "league/csv": "^9.9",
        "nikic/iter": "^2.2",
        "spatie/laravel-package-tools": "^1.14.0",
        "vhood/tree-converter": "^1.1"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-dev-tools": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Parables\\Geo\\": "src/",
            "Parables\\Geo\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Parables\\Geo\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Parables\\Geo\\GeoServiceProvider"
            ],
            "aliases": {
                "Geo": "Parables\\Geo\\Facades\\Geo"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
